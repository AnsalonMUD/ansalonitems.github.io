@import 'yeticss'

html, body
  padding: 0
  margin: 0

header
  height: 5em
  line-height: 5em
  background-color: #438193

  .container
    height: 100%

  h1
    display: inline-block
    vertical-align: middle
    margin: 0

    a, a:hover, a:active, a:visited, a:focus
      color: black

section
  min-height: 20em

footer
  padding: 1em 0
  color: #666
  font-size: .8em
  text-align: center;

pre
  margin: 0
  padding: 0
